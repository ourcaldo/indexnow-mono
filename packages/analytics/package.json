{
    "name": "@indexnow/analytics",
    "version": "1.0.0",
    "private": true,
    "sideEffects": false,
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js",
            "default": "./dist/index.js"
        }
    },
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "clean": "node -e \"require('fs').rmSync('dist',{recursive:true,force:true})\"",
        "lint": "eslint .",
        "type-check": "tsc --noEmit"
    },
    "dependencies": {
        "@indexnow/shared": "workspace:*",
        "@sentry/nextjs": "^8.55.0",
        "posthog-js": "^1.335.2",
        "analytics": "^0.8.19",
        "@analytics/google-analytics": "^1.1.0",
        "@analytics/google-tag-manager": "^0.6.0",
        "@analytics/customerio": "^0.2.2"
    },
    "devDependencies": {
        "@indexnow/eslint-config": "workspace:*",
        "tsup": "^8.0.1",
        "typescript": "^5.6.3"
    }
}
