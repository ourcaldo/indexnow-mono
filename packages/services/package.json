{
  "name": "@indexnow/services",
  "version": "1.0.0",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "node -e \"require('fs').rmSync('dist',{recursive:true,force:true})\"",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@indexnow/database": "workspace:*",
    "@indexnow/shared": "workspace:*",
    "@supabase/supabase-js": "^2.54.0"
  },
  "devDependencies": {
    "@indexnow/eslint-config": "workspace:*",
    "@types/node": "^20.0.0",
    "eslint": "^9",
    "tsup": "^8.0.1",
    "typescript": "^5.6.3"
  }
}
