{
  "name": "@indexnow/supabase-client",
  "version": "1.0.0",
  "private": true,
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "node -e \"require('fs').rmSync('dist',{recursive:true,force:true})\"",
    "lint": "eslint .",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@indexnow/shared": "workspace:*",
    "@supabase/supabase-js": "^2.54.0",
    "@supabase/ssr": "^0.6.1"
  },
  "devDependencies": {
    "@indexnow/eslint-config": "workspace:*",
    "@types/react": "^19.2.10",
    "tsup": "^8.0.1",
    "typescript": "^5.6.3"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  }
}
